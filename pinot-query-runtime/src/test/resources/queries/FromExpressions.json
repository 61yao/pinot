{
  "basic_join_queries": {
    "tables": {
      "tbl1" : {
        "schema": [
          {"name": "num", "type": "INT"},
          {"name": "name", "type": "STRING"}
        ],
        "inputs": [
          [1, "a"],
          [2, "b"],
          [3, "c"]
        ]
      },
      "tbl2" : {
        "schema": [
          {"name": "num", "type": "INT"},
          {"name": "value", "type": "STRING"}
        ],
        "inputs": [
          [1, "xxx"],
          [3, "yyy"],
          [5, "zzz"]
        ]
      }
    },
    "queries": [
      {
        "psql": "7.2.1.1",
        "description": "LEFT OUTER JOIN",
        "sql": "SELECT * FROM {tbl1} LEFT OUTER JOIN {tbl2} ON {tbl1}.num = {tbl2}.num"
      }
    ]
  }
}
